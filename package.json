{
  "private": true,
  "name": "xeho91-logo",
  "version": "1.0.0",
  "description": "xeho91's logo",
  "files": [
    "./build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeho91/logo.git"
  },
  "keywords": [
    "logo",
    "brand",
    "assets",
    "SVG",
    "animated",
    "SMIL"
  ],
  "author": "Mateusz Kadlubowski <xeho91@pm.me> https://xeho91.site/",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xeho91/logo/issues"
  },
  "homepage": "https://github.com/xeho91/logo#readme",
  "scripts": {
    "start": "chokidar \"source/*.svg\" --command \"node scripts/buildSVG.js {path}\"",
    "build": "node scripts/buildSVG.js \"./source/xeho91-logo.svg\" \"./source/xeho91-logo.animated.svg\"",
    "export:png": "svgexport \"./build/xeho91-logo.min.svg\" \"./build/xeho91-logo.png\" 1920:1350",
    "minify": "svgo --config \"config/.svgorc.js\" --folder \"./build\" --output \"./build/minified\""
  },
  "devDependencies": {
    "chokidar": "^3.5.1",
    "chokidar-cli": "^2.1.0",
    "posthtml": "^0.15.1",
    "posthtml-transform": "^1.0.10",
    "svgexport": "^0.4.1",
    "svgo": "2.1.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@xeho91/colors": "github:xeho91/colors#main"
  }
}
